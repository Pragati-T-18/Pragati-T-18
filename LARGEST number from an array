DATA SEGMENT 
    ARRAY DB 15H,45H,08H,56H,78H 
    LARGEST DB 00H 
DATA ENDS 

CODE SEGMENT 
ASSUME CS:CODE,DS:DATA

START:
   MOV DX,DATA 
   MOV DS,DX
   MOV CX,04H 
   LEA SI, ARRAY 
   MOV AL,[SI] 
UP: 
   INC SI 
   CMP AL,[SI] 
   JG NEXT 
   MOV AL,[SI] 
NEXT:
   DEC CX 
   JNZ UP
   MOV LARGEST,AL 

   MOV AH, 4CH 
   INT 21H 
CODE ENDS 
