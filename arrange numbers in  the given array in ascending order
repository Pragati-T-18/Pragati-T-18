DATA SEGMENT
  ARRAY DB 10H,11H,12H,13H,14H 
DATA ENDS

CODE SEGMENT
START:
  ASSUME CS:CODE,DS:DATA
  MOV DX,DATA
  MOV DS,DX
  MOV BL,04H
LOOP1:
  LEA SI, ARRAY 
  MOV CL,04H
LOOP2:
  MOV AL,[SI]
  CMP AL,[SI+1]
  JNG DOWN
  XCHG AL,[SI+1]
  XCHG AL,[SI]
 DOWN: 
  INC SI
  DEC CL
  JNZ LOOP2
  DEC BL
  JNZ LOOP1

CODE ENDS
END START
